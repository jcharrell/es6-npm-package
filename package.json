{
  "name": "es6-npm-package",
  "version": "0.1.0",
  "description": "An ES6 npm module boilerplate which will transpile to ES5 when publishing.  Start coding and stop worrying!",
  "main": "dist/main.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --require co-mocha",
    "prepublish": "./node_modules/.bin/babel -d dist src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jcharrell/es6-npm-package.git"
  },
  "keywords": [
    "npm",
    "boilerplate",
    "starter",
    "generators"
  ],
  "author": "Chris Harrell",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "co-mocha": "^1.1.2",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "co": "^4.6.0"
  }
}
